    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "node-color-log": "^12.0.0",
    "optics-ts": "^2.4.1",
    "prisma": "^5.13.0",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.2.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "reflect-metadata": "^0.2.2",
    "remeda": "^1.61.0",
    "rxjs": "^7.8.1",
