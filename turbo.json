{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["DISPLAY", "VITE_*"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
