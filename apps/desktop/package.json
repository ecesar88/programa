    "react-icons": "^5.2.1",
    "react-router": "^7.0.2",
    "react-toastify": "^10.0.5",
    "remeda": "^2.6.0",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^2.4.0",
    "tinycolor2": "^1.6.0",
