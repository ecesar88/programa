    "electron-log": "^5.1.6",
    "electron-vite": "^2.3.0",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.34.4",
    "graphql-codegen-fragment-matcher": "0.18.2",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
